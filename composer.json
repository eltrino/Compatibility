{
    "name": "eltrino/compatibility",
    "type": "magento-module",
    "license": "OSL-3.0",
    "homepage": "https://bitbucket.org/eltrino/compatibility",
    "description": "Module provide backward compatibility with Magento 1.x for Magento 2 modules",
    "authors": [
        {
            "name": "Eltrino LLC",
            "email": "license@eltrino.com"
        }
    ],
    "require": {
        "magento-hackathon/magento-composer-installer": "*"
    },
    "extra": {
        "map": [
            [
                "app/code/community/Eltrino/Compatibility",
                "app/code/community/Eltrino/Compatibility"
            ],
            [
                "app/code/community/Magento",
                "app/code/community/Magento"
            ],
            [
                "app/etc/modules/Eltrino_Compatibility.xml",
                "app/etc/modules/Eltrino_Compatibility.xml"
            ]
        ]
    }
}